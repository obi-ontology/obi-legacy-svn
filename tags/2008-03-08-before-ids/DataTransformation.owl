  <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_200031">
    <curation_status xml:lang="en">ready_for_release</curation_status>
    <definition xml:lang="en">A data transformation whose input consists of expression levels of entities (such as transcripts or proteins), or of sets of such expression levels, under two or more conditions and whose output reflects which of these are likely to have different expression across such conditions</definition>
    <definition_editor xml:lang="en">Elisabetta Manduchi</definition_editor>
    <preferred_term xml:lang="en">differential expression analysis</preferred_term>
    <definition_citation xml:lang="en">PERSON: Elisabetta Manduchi</definition_citation>
    <example_of_usage xml:lang="en">Analyses implemented by the SAM (http://www-stat.stanford.edu/~tibs/SAM), PaGE (www.cbil.upenn.edu/PaGE) or GSEA (www.broad.mit.edu/gsea/) algorithms and software</example_of_usage>
    <rdfs:label xml:lang="en">differential expression analysis</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://obi.sourceforge.net/ontology/OBI.owl#OBI_200000"/>
  </owl:Class>
  <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_200049">
