  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0000278">
    <rdfs:label xml:lang="en">disease_stage</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.ifomis.org/bfo/1.1/span#FiatProcessPart"/>
    <OBI_0000291 xml:lang="en">is a fiat_process_part which is part_of a disease_process which is associated with position in the normal progression of the disease</OBI_0000291>
    <OBI_0000287 xml:lang="en">Stage II breast cancer, The timepoint of recovery from a disease</OBI_0000287>
  </owl:Class>
  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0000076">
    <OBI_0000287 xml:lang="en">Earthquake that destroys the lab, an outside investigator discovering an adverse effect of the reagants used</OBI_0000287>
    <OBI_0000291 xml:lang="en">An extraneous process is a process which is external in origin to the investigation that has an impact on the outcome.</OBI_0000291>
    <rdfs:label xml:lang="en">unplanned_process_part</rdfs:label>
    <OBI_0000279 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >OBI</OBI_0000279>
    <rdfs:subClassOf rdf:resource="http://www.ifomis.org/bfo/1.1/span#FiatProcessPart"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0000138">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0000137"/>
    </rdfs:subClassOf>
    <OBI_0000291 xml:lang="en">An in-vivo state is a state in which the entity remains part of the living organism.</OBI_0000291>
    <OBI_0000279 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >OBI</OBI_0000279>
    <rdfs:label xml:lang="en">in-vivo_state</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0000072">
    <OBI_0000291 xml:lang="en">A parameter value application is part of a protocol application in which the values of the parameter are set.</OBI_0000291>
    <OBI_0000279 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >OBI</OBI_0000279>
    <rdfs:label xml:lang="en">parameter_value_application</rdfs:label>
    <OBI_0000287 xml:lang="en">Setting the centrifuge to 1200 RPM.</OBI_0000287>
    <rdfs:subClassOf rdf:resource="http://www.ifomis.org/bfo/1.1/span#FiatProcessPart"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0000155">
    <rdfs:label xml:lang="en">disease</rdfs:label>
    <OBI_0000291 xml:lang="en">Place_holder for disease ontology</OBI_0000291>
    <OBI_0000275 xml:lang="en">rem1: BP - placed into quality. needs to be discussed</OBI_0000275>
    <OBI_0000275 xml:lang="en">rem0: BS did not view disease as a process, and indeed it is now a child of dependent continuant.</OBI_0000275>
    <rdfs:subClassOf rdf:resource="http://www.ifomis.org/bfo/1.1/snap#Disposition"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0000137">
    <OBI_0000291 xml:lang="en">a state is a quality indicating the condition in which an entity exists, e.g. immortalized, frozen, cellular_state (in-vivo, in-vitro), macromolecular state, 'ectopic in-vivo', 'denatured or native for protein'.</OBI_0000291>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0000334"/>
    </rdfs:subClassOf>
    <OBI_0000279 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >OBI</OBI_0000279>
    <rdfs:label xml:lang="en">state</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0000259">
    <OBI_0000275 xml:lang="en">rem0: (AL) One of in-vitro state/ex-vivo state should be deleted, and made a synonym of the remaining term.</OBI_0000275>
    <OBI_0000291 xml:lang="en">An ex-vivo state is a state in which the entity is in an artificial environment outside the living organism. definition_source: http://wordnet.princeton.edu/perl/webwn?s=in%20vitro, Jan 3, 2007.</OBI_0000291>
    <rdfs:label xml:lang="en">ex-vivo_state</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.obofoundry.org/obo/OBI_0000137"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0000044">
    <OBI_0000279 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >OBI</OBI_0000279>
    <OBI_0000275 xml:lang="en">BP: moved and renamed for BFO</OBI_0000275>
    <OBI_0000275 xml:lang="en">information_content_entity "is_encoded_in" some physical_document in obi before split (040907).</OBI_0000275>
    <OBI_0000291 xml:lang="en">A physical document is an object serving as a record of information by means of symbolic marks.</OBI_0000291>
    <rdfs:label xml:lang="en">physical_document</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.obofoundry.org/obo/OBI_0000141"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0000334">
    <rdfs:label xml:lang="en">_to_be_fixed</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0000354">
    <rdfs:label xml:lang="en">disease_course</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.ifomis.org/bfo/1.1/span#Process"/>
    <OBI_0000291 xml:lang="en">is a process through which a disease manifests itself</OBI_0000291>
  </owl:Class>
  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0000033">
    <rdfs:comment xml:lang="en">A variable in an experiment that is designed to look at differences between organisms of different genders, mating types etc.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.obofoundry.org/obo/OBI_0000334"/>
    <rdfs:label xml:lang="en">sex_effect</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0000130">
    <OBI_0000291 xml:lang="en">An in-vitro state is a state in which the entity is in an artificial environment outside the living organism. definition_source: http://wordnet.princeton.edu/perl/webwn?s=in%20vitro, Jan 3, 2007.</OBI_0000291>
    <rdfs:label xml:lang="en">in-vitro_state</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.obofoundry.org/obo/OBI_0000137"/>
  </owl:Class>
