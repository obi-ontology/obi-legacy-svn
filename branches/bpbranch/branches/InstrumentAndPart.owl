    <rdfs:label xml:lang="en">power supply</rdfs:label>
    <OBI_0000287 xml:lang="en">A AC/DC transformer that generates the reqired power for an electrophoresis apparatus</OBI_0000287>
    <OBI_0000286 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >PSU</OBI_0000286>
    <OBI_0000286 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
