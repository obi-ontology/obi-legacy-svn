    <OBI_0000288 xml:lang="en">Guava_Personal_Cell_Analysis</OBI_0000288>
  </owl:Class>
  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0400069">
    <OBI_0000288 xml:lang="en">Luminex_100</OBI_0000288>
    <OBI_0000279 xml:lang="en">http://www.luminexcorp.com/products/luminex_100IS.html</OBI_0000279>
