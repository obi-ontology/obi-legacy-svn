  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0400059">
    <OBI_0000287 xml:lang="en">Interrogation point in BD FACS Calibur</OBI_0000287>
    <OBI_0000281 rdf:resource="http://purl.obofoundry.org/obo/OBI_0000320"/>
    <OBI_0000288 xml:lang="en">interrogation_point</OBI_0000288>
    <OBI_0000275 xml:lang="en">DS: Spatial entity?
AI: Ryan goes back to John Quinn and saks for usage example of this term.</OBI_0000275>
    <rdfs:label xml:lang="en">interrogation_point</rdfs:label>
    <OBI_0000291 xml:lang="en">Point within the fluidic subsystem at which the laser intersects the stream, illuminating the cells where they emit their fluorescence and scatter the light. The laser irratidates the cell at the interogation point.</OBI_0000291>
    <rdfs:subClassOf rdf:resource="http://www.ifomis.org/bfo/1.1/snap#ThreeDimensionalRegion"/>
    <OBI_0000274 xml:lang="en">John Quinn</OBI_0000274>
    <OBI_0000279 xml:lang="en">http://www.flocyte.com/FRTP/Resources/flow_cytometry_glossary.htm, 2007-05-11</OBI_0000279>
  </owl:Class>
