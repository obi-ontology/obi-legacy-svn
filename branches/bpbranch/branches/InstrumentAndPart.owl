    <OBI_0000288 xml:lang="en">BioSorter250</OBI_0000288>
    <OBI_0000287 xml:lang="en">BioSorter 250 at TSRI Flow Cytometry Core Facility</OBI_0000287>
    <OBI_0000281 rdf:resource="http://purl.obofoundry.org/obo/OBI_0000318"/>
