    <rdfs:label xml:lang="en">heating_block</rdfs:label>
    <OBI_0000279 xml:lang="en">MO</OBI_0000279>
    <OBI_0000287 xml:lang="en">An instrument used to heat and/or maintain material at a set temperature.</OBI_0000287>
    <OBI_0000288 xml:lang="en">heating_block</OBI_0000288>
  </owl:Class>
  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0400053">
