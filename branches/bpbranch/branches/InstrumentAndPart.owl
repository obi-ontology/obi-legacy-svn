    <OBI_0000288 xml:lang="en">photomultiplier_tube</OBI_0000288>
    <OBI_0000287 xml:lang="en">R9647 by manufactured by Hamamatsu</OBI_0000287>
    <OBI_0000274 xml:lang="en">John Quinn</OBI_0000274>
    <OBI_0000281 rdf:resource="http://purl.obofoundry.org/obo/OBI_0000320"/>
