    <OBI_0000274 xml:lang="en">Daniel Schober</OBI_0000274>
    <OBI_0000287 xml:lang="en">example_of_usage to be added</OBI_0000287>
    <OBI_0000281 rdf:resource="http://purl.obofoundry.org/obo/OBI_0000328"/>
    <OBI_0000291 xml:lang="en">The Supernatant Collection system is designed for collecting 90% of the supernatant in a microplate well and separating the living cell with no stress, eliminating centrifugation and other similar techniques. It can be used in a variety of release assays with different radioactive isotopes, such as Cr51 or I125.</OBI_0000291>
  </owl:Class>
  <owl:Class rdf:about="http://purl.obofoundry.org/obo/OBI_0400075">
