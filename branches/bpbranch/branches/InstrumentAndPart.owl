    <rdfs:label xml:lang="en">incubator</rdfs:label>
    <OBI_0000291 xml:lang="en">An incubator is an instrument in which environmental conditions (light, photoperiod, temperature, humidity, etc.) can be controlled</OBI_0000291>
    <OBI_0000288 xml:lang="en">incubator</OBI_0000288>
    <rdfs:subClassOf>
