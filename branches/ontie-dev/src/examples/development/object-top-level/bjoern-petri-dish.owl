<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY owl11 "http://www.w3.org/2006/12/owl11#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl11xml "http://www.w3.org/2006/12/owl11-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY unnamed "http://www.owl-ontologies.com/unnamed.owl#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.owl-ontologies.com/unnamed.owl#"
     xml:base="http://www.owl-ontologies.com/unnamed.owl"
     xmlns:owl11="http://www.w3.org/2006/12/owl11#"
     xmlns:owl11xml="http://www.w3.org/2006/12/owl11-xml#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:unnamed="http://www.owl-ontologies.com/unnamed.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about=""/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.owl-ontologies.com/unnamed.owl#forms -->

    <owl:ObjectProperty rdf:about="#forms">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <owl:inverseOf rdf:resource="#is_formed_from"/>
        <rdfs:subPropertyOf rdf:resource="#is_granular_part"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Relation between Amount of matter and physical object in the cognitivist / multiplicative formulation of an ontology.</rdfs:comment>
        <rdfs:domain rdf:resource="#Collective_amount_of_matter"/>
        <rdfs:range rdf:resource="#Physical_object"/>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#has_determinate_part -->

    <owl:ObjectProperty rdf:about="#has_determinate_part">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <owl:inverseOf rdf:resource="#is_determinate_part_of"/>
        <rdfs:range rdf:resource="#Physical_entity"/>
        <rdfs:subPropertyOf rdf:resource="#has_gross_part"/>
        <rdfs:domain rdf:resource="#Physical_object"/>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#has_grain -->

    <owl:ObjectProperty rdf:about="#has_grain">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:domain rdf:resource="#Collective_amount_of_matter"/>
        <rdfs:subPropertyOf rdf:resource="#has_granular_part"/>
        <rdfs:range rdf:resource="#Physical_object"/>
        <owl:inverseOf rdf:resource="#is_grain_of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#has_granular_part -->

    <owl:ObjectProperty rdf:about="#has_granular_part">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <owl:inverseOf rdf:resource="#is_granular_part"/>
        <rdfs:subPropertyOf rdf:resource="#has_part"/>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#has_gross_part -->

    <owl:ObjectProperty rdf:about="#has_gross_part">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:subPropertyOf rdf:resource="#has_part"/>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#has_ingredient -->

    <owl:ObjectProperty rdf:about="#has_ingredient">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:range rdf:resource="#Collective_amount_of_matter"/>
        <rdfs:domain rdf:resource="#Collective_amount_of_matter"/>
        <rdfs:subPropertyOf rdf:resource="#has_gross_part"/>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#has_part -->

    <owl:ObjectProperty rdf:about="#has_part">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <owl:inverseOf rdf:resource="#is_part_of"/>
        <rdfs:subPropertyOf rdf:resource="#inverse_relational_property"/>
        <rdfs:range rdf:resource="#Physical_entity"/>
        <rdfs:domain rdf:resource="#Physical_entity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#inverse_relational_property -->

    <owl:ObjectProperty rdf:about="#inverse_relational_property">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <owl:inverseOf rdf:resource="#relational_property"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >See documentation for relational properties.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#is_determinate_part_of -->

    <owl:ObjectProperty rdf:about="#is_determinate_part_of">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Countable discrete parts such that the whole is diminished by the loss of one of them.  See paper</rdfs:comment>
        <rdfs:range rdf:resource="#Physical_object"/>
        <rdfs:subPropertyOf rdf:resource="#is_gross_part_of"/>
        <rdfs:domain rdf:resource="#Physical_entity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#is_formed_from -->

    <owl:ObjectProperty rdf:about="#is_formed_from">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:domain rdf:resource="#Physical_object"/>
        <rdfs:subPropertyOf rdf:resource="#has_granular_part"/>
        <rdfs:range rdf:resource="#Collective_amount_of_matter"/>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#is_grain_of -->

    <owl:ObjectProperty rdf:about="#is_grain_of">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:domain rdf:resource="#Physical_object"/>
        <rdfs:range rdf:resource="#Collective_amount_of_matter"/>
        <rdfs:subPropertyOf rdf:resource="#is_granular_part"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation between a grain and a collective.  

The boundary property that indicates a shift in level of collectivity (&quot;granularity&quot;). 

The loss of ndividual grains does not diminish the whole. Collectives have emergent properties not directly predictable from the grainsl.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#is_granular_part -->

    <owl:ObjectProperty rdf:about="#is_granular_part">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:subPropertyOf rdf:resource="#is_part_of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#is_gross_part_of -->

    <owl:ObjectProperty rdf:about="#is_gross_part_of">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:subPropertyOf rdf:resource="#is_part_of"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The parts including ingredients, constituent parts and determinatge parts, but not granular parts.</rdfs:comment>
        <owl:inverseOf rdf:resource="#has_gross_part"/>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#is_ingredient_of -->

    <owl:ObjectProperty rdf:about="#is_ingredient_of">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:subPropertyOf rdf:resource="#is_gross_part_of"/>
        <rdfs:domain rdf:resource="#Collective_amount_of_matter"/>
        <rdfs:range rdf:resource="#Collective_amount_of_matter"/>
        <owl:inverseOf rdf:resource="#has_ingredient"/>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#is_part_of -->

    <owl:ObjectProperty rdf:about="#is_part_of">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:domain rdf:resource="#Physical_entity"/>
        <rdfs:range rdf:resource="#Physical_entity"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The parent generic part-whole relation

A transitive relation.  Neither antisymmetry nor reflexivity can be represented.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#relational_property"/>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#relational_property -->

    <owl:ObjectProperty rdf:about="#relational_property">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Parent relational property - 

as opposed to refining properties (roughly modifiers) 

as opposed to &apos;attributive properties&apos; the properties required to link entities to their attributes in translations of UML models - roughly but not always refining properties.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.owl-ontologies.com/unnamed.owl#Collective_amount_of_matter -->

    <owl:Class rdf:about="#Collective_amount_of_matter">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="#has_grain"/>
                                <owl:someValuesFrom rdf:resource="#Physical_object"/>
                            </owl:Restriction>
                            <rdf:Description rdf:about="#Physical_entity"/>
                        </owl:intersectionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#has_gross_part"/>
                        <owl:allValuesFrom rdf:resource="#Collective_amount_of_matter"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >From DOLCE.  This version of the ontology takes a &quot;Cognitivist&quot; or &quot;Multicative&quot; view.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#Domain_entity -->

    <owl:Class rdf:about="#Domain_entity">
        <rdfs:comment rdf:datatype="&xsd;string"
            >A convnient top entithy separating off other purely implementational classes etc. 

The ontology below is a severely abbreviated version of a roughly DOLCE compatible upper ontology supplemented by collectivity.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#Physical_entity -->

    <owl:Class rdf:about="#Physical_entity">
        <rdfs:subClassOf rdf:resource="#Domain_entity"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Physical as opposed to non-physical entities.  This demo ontology omits non-physical entities.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#Physical_object -->

    <owl:Class rdf:about="#Physical_object">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Physical_entity"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#has_gross_part"/>
                        <owl:someValuesFrom rdf:resource="#Physical_object"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#Collective_amount_of_matter"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Phsical objects with mass etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#Probe -->

    <owl:Class rdf:about="#Probe">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Probes are classes included to test the ontology.  They are all defined and should have the correct entities under them when classified.

The debugging framework in Protege OWL can be set up to test probes systematically.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#Probe_Object_petri_dish_part -->

    <owl:Class rdf:about="#Probe_Object_petri_dish_part">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#is_gross_part_of"/>
                        <owl:someValuesFrom rdf:resource="#petri_dish"/>
                    </owl:Restriction>
                    <rdf:Description rdf:about="#Physical_object"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#Probe"/>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#Probe_has_part_plastic_molecule -->

    <owl:Class rdf:about="#Probe_has_part_plastic_molecule">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_granular_part"/>
                <owl:someValuesFrom rdf:resource="#plastic_molecule"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#Probe"/>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#Probe_plastic_object -->

    <owl:Class rdf:about="#Probe_plastic_object">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#is_formed_from"/>
                <owl:someValuesFrom rdf:resource="#plastic"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#Probe"/>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#petri_dish -->

    <owl:Class rdf:about="#petri_dish">
        <rdfs:subClassOf rdf:resource="#Physical_object"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#is_formed_from"/>
                <owl:someValuesFrom rdf:resource="#plastic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#is_formed_from"/>
                <owl:allValuesFrom rdf:resource="#plastic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#plastic -->

    <owl:Class rdf:about="#plastic">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#has_grain"/>
                        <owl:allValuesFrom rdf:resource="#plastic_molecule"/>
                    </owl:Restriction>
                    <rdf:Description rdf:about="#Collective_amount_of_matter"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#plastic_molecule -->

    <owl:Class rdf:about="#plastic_molecule">
        <rdfs:subClassOf rdf:resource="#Physical_object"/>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.owl-ontologies.com/unnamed.owl#petri_dish_1 -->

    <petri_dish rdf:about="#petri_dish_1"/>
    


    <!-- http://www.owl-ontologies.com/unnamed.owl#piece_of_petri_dish_1 -->

    <rdf:Description rdf:about="#piece_of_petri_dish_1">
        <has_determinate_part rdf:resource="#piece_of_petri_dish_1"/>
    </rdf:Description>
</rdf:RDF>
