    <preferred_term rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >stopping_rule</preferred_term>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_500021"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">stopping_rule</rdfs:label>
  </owl:Class>
