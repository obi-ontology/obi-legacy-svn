  <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#number">

<!--    <rdfs:subClassOf rdf:resource="http://obi.sourceforge.net/ontology/OBI.owl#information_content_entity"/>
 -->
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
>number</rdfs:label>

    <definition_editor rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
>PERSON:Chris Stoeckert</definition_editor>

    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
>A number is an information content entity which provides a magnitude for the result of a measurement.</definition>

    <editor_note rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
>Was 'value'</editor_note>

  </owl:Class>

  <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_900004">
    <rdfs:label xml:lang="en">instrument part function</rdfs:label>
    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A function inhering the cardinal part of an instrument necessary for that instrument to participate in critical process in the context of a specific investigation plan.</definition>
    <definition_editor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bill Bug</definition_editor>
    <curation_status rdf:datatype="http://www.w3.org/2001/XMLSchema#string">definition_incomplete</curation_status>
    <preferred_term rdf:datatype="http://www.w3.org/2001/XMLSchema#string">instrument part function</preferred_term>
    <definition_citation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PERSON:William Bug@2007/07/10</definition_citation>
    <example_of_usage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">E.g. the function of the electromagnetic lenses in an electron microscope to direct the incident electron beam trajectory so as to systematically raster across a specimen when constructing the overall image; the function of Nomarksi optics to enhance the constrast of specimens viewed in a light microscope based on their ability to refract visible wavelengths of light.</example_of_usage>
    <rdfs:subClassOf>
      <!-- <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_900000"/> -->
      <owl:Class rdf:about="http://www.geneontology.org/formats/oboInOwl#ObsoleteClass"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_900001">
    <rdfs:label xml:lang="en">reagent function</rdfs:label>
    <definition_citation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PERSON:William Bug@2007/07/10</definition_citation>
    <curation_status rdf:datatype="http://www.w3.org/2001/XMLSchema#string">definition_incomplete</curation_status>
    <preferred_term rdf:datatype="http://www.w3.org/2001/XMLSchema#string">reagent function</preferred_term>
    <definition_editor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bill Bug</definition_editor>
    <example_of_usage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">E.g. the function of Cy5 coupled-ligands to enable separation of cells in a Fluorescence-Activated Cell Sorter (FACS) by virtue of cells possessing specific receptors with an specific affinity to bind the coupled ligand thereby causing only those cells to fluoresce when illuminated with laser light frequency that specifically excites Cy5, triggering relays and redirecting the solution flow to a separate effluent container.</example_of_usage>
    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A function inhering a molecule, set of molecules, cell, collection of cells, or tissue  enabling it to participate in critical material transformation in the context of a specific investigation plan.</definition>
    <rdfs:subClassOf>
      <!-- <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_900000"/> -->
      <owl:Class rdf:about="http://www.geneontology.org/formats/oboInOwl#ObsoleteClass"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_900007">
    <definition_citation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PERSON:William Bug@2007/07/10</definition_citation>
    <preferred_term rdf:datatype="http://www.w3.org/2001/XMLSchema#string">dialysis tubing function</preferred_term>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">dialysis tubing function</rdfs:label>
    <preferred_term rdf:datatype="http://www.w3.org/2001/XMLSchema#string">dialysis tubing function</preferred_term>
    <rdfs:subClassOf>
      <!-- <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_900005"/> -->
      <owl:Class rdf:about="http://www.geneontology.org/formats/oboInOwl#ObsoleteClass"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_900005">
    <definition_editor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bill Bug</definition_editor>
    <curation_status rdf:datatype="http://www.w3.org/2001/XMLSchema#string">definition_incomplete</curation_status>
    <preferred_term rdf:datatype="http://www.w3.org/2001/XMLSchema#string">labware function</preferred_term>
    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A function inhering in a type of labware enabling it to participate in critical process in the context of a specific investigation plan.</definition>
    <definition_citation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PERSON:William Bug@2007/07/10</definition_citation>
    <editor_note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">How does this relate to an object such as the enzyme-coated plate used in an ELISA protocol application?  The plate itself is a type of labware, but the enzyme is a reagent.(BB:2007-07-10).</editor_note>
    <rdfs:label xml:lang="en">labware function</rdfs:label>
    <example_of_usage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">E.g. the function of semi-permeable dialysis tubing to separate solutes based on size by virtue of restricted diffusion and osmosis; the function of a plastic 25 ml centrifuge tube to act as an inert storage vessel for a material sample.</example_of_usage>
    <rdfs:subClassOf>
      <!-- <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_900000"/> -->
      <owl:Class rdf:about="http://www.geneontology.org/formats/oboInOwl#ObsoleteClass"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_900003">
    <example_of_usage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">E.g. the function of a high pressure liquid chromatagraphic (HPLC) system to separate molecules based on their solubility properties</example_of_usage>
    <rdfs:label xml:lang="en">instrument platform function</rdfs:label>
    <preferred_term rdf:datatype="http://www.w3.org/2001/XMLSchema#string">instrument platform function</preferred_term>
    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A function inhering an instrument_platform enabling it to participate in critical process in the context of a specific investigation plan.</definition>
    <definition_editor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bill Bug</definition_editor>
    <definition_citation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PERSON:William Bug@2007/07/10</definition_citation>
    <curation_status rdf:datatype="http://www.w3.org/2001/XMLSchema#string">definition_incomplete</curation_status>
    <rdfs:subClassOf>
      <!-- <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_900000"/> -->
      <owl:Class rdf:about="http://www.geneontology.org/formats/oboInOwl#ObsoleteClass"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_900006">
    <curation_status rdf:datatype="http://www.w3.org/2001/XMLSchema#string">definition_incomplete</curation_status>
    <definition_citation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PERSON:William Bug@2007/07/10</definition_citation>
    <example_of_usage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">E.g. the function of a spectrophotometer to provide a measure of the relative absorbance of a liquid sample for a particular wavelength (or set of wavelengths) of light.</example_of_usage>
    <definition_editor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bill Bug</definition_editor>
    <rdfs:label xml:lang="en">analytical instrument function</rdfs:label>
    <preferred_term rdf:datatype="http://www.w3.org/2001/XMLSchema#string">analytical instrument function</preferred_term>
    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An instrument function where the process in which the instrument participates results in a quality measurement of the material under examination which is critical in the context of a specific investigation plan.</definition>
    <rdfs:subClassOf>
      <!-- <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_303"/> -->
      <owl:Class rdf:about="http://www.geneontology.org/formats/oboInOwl#ObsoleteClass"/>
    </rdfs:subClassOf>
  </owl:Class>
