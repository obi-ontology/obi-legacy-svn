  <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#format_standard">
    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a format standard is a digital entity that consists of rules applied to information, as opposed to rules applied to objects (which are termed plans)</definition>
    <definition_editor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PERSON:Chris Stoeckert</definition_editor>
    <definition_editor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PERSON:Jennifer Fostel</definition_editor>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">format standard</rdfs:label>
<!--    <rdfs:subClassOf rdf:resource="http://obi.sourceforge.net/ontology/OBI.owl#digital_entity"/> -->
   <rdfs:subClassOf rdf:resource="http://www.geneontology.org/formats/oboInOwl#ObsoleteClass"/>
