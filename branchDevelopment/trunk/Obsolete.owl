    <rdfs:subClassOf rdf:resource="http://www.geneontology.org/formats/oboInOwl#ObsoleteClass"/>
    <rdfs:label xml:lang="en">funder</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_289">
    <rdfs:label xml:lang="en">reporter_role</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.geneontology.org/formats/oboInOwl#ObsoleteClass"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >this role was added as a sibling of reporter, which is not sensible.  we will keep one term for reporter, and use the one with a definition provided.  The original term contained a child "dye-role" which is equivalent to label role</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://obi.sourceforge.net/ontology/OBI.owl#OBI_9999998">
