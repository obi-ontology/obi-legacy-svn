F. Wilkins and S. A. Hardy and L. Boddy and C. W. Morris},	year={2001},	month={Jul 1},	title={Comparison of five clustering algorithms to classify phytoplankton from flow cytometry data},	journal={Cytometry : the journal of the Society for Analytical Cytology},	volume={44},	number={3},	pages={210-217},	note={LR: 20041117; CI: Copyright 2001; JID: 8102328; ppublish},	abstract={BACKGROUND: Artificial neural networks (ANNs) have been shown to be valuable in the analysis of analytical flow cytometric (AFC) data in aquatic ecology. Automated extraction of clusters is an important first stage in deriving ANN training data from field samples, but AFC data pose a number of challenges for many types of clustering algorithm. The fuzzy k-means algorithm recently has been extended to address nonspherical clusters with the use of scatter matrices. Four variants were proposed, each optimizing a different measure of clustering "goodness." METHODS: With AFC data obtained from marine phytoplankton species in culture, the four fuzzy k-means algorithm variants were compared with each other and with another multivariate clustering algorithm based on critical distances currently used in flow cytometry. RESULTS: One of the algorithm variants (adaptive distances, also known as the Gustafson--Kessel algorithm) was found to be robust and reliable, whereas the others showed various problems. CONCLUSIONS: The adaptive distances algorithm was superior in use to the clustering algorithms against which it was tested, but the problem of automatic determination of the number of clusters remains to be addressed.},	keywords={Algorithms; Automatic Data Processing; Comparative Study; Flow Cytometry/methods; Forecasting; Neural Networks (Computer); Phytoplankton/classification; Research; Research Support, Non-U.S. Gov't},	isbn={0196-4763},	language={eng}}}@article{RefWorks:1407,	author={J. N. Winter and E. A. Weller and S. J. Horning and M. Krajewska and D. Variakojis and T. M. Habermann and R. I. Fisher and P. J. Kurtin and W. R. Macon and M. Chhanabhai and R. E. Felgar and E. D. Hsi and L. J. Medeiros and J. K. Weick and J. C. Reed and R. D. Gascoyne},	year={2006},	month={Jun 1},	title={Prognostic significance of Bcl-6 protein expression in DLBCL treated with CHOP or R-CHOP: a prospective correlative study},	journal={Blood},	volume={107},	number={11},	pages={4207-4213},	note={LR: 20071115; PUBM: Print-Electronic; GR: CA04949/CA/United States NCI; GR: CA11083/CA/United States NCI; GR: CA13650/CA/United States NCI; GR: CA17145/CA/United States NCI; GR: CA21115/CA/United States NCI; GR: CA23318/CA/United States NCI; GR: CA32102/CA/United States NCI; GR: CA38926/CA/United States NCI; GR: CA66636/CA/United States NCI; GR: CA92291/CA/United States NCI; DEP: 20060131; JID: 7603509; 0 (Antibodies, Monoclonal); 0 (BCL6 protein, human); 0 (CHOP protocol); 0 (DNA-Binding Proteins); 0 (rituximab); 23214-92-8 (Doxorubicin); 50-18-0 (Cyclophosphamide); 53-03-2 (Prednisone); 57-22-7 (Vincristine); CIN: Blood. 2007 Jan 15;109(2):843-4; discussion 844-5. PMID: 17210865; EIN: Blood. 2007 Mar 15;109(6):2292; PMC1895783; 2006/01/31 [aheadofprint]; ppublish},	abstract={Bcl-6 protein expression, a marker of germinal center origin, has been associated with a favorable prognosis in diffuse large B-cell lymphoma (DLBCL). To determine the prognostic significance of this marker when rituximab (R) was added to cyclophosphamide, doxorubicin, vincristine, and prednisone (CHOP) chemotherapy, we prospectively studied Bcl-6 protein expression by immunohistochemical staining of 199 paraffin-embedded specimens from patients enrolled in the US Intergroup phase 3 trial comparing R-CHOP to CHOP with or without maintenance R. In Bcl-6(-) patients, failure-free survival (FFS) and overall survival (OS) were prolonged for those treated with R-CHOP alone compared to CHOP alone (2-year FFS 76% versus 9%, P < .001; 2-year OS 79% versus 17%, P < .001). In contrast, no differences in FFS and OS were detected between treatment arms for Bcl-6(+) cases. In the multivariate analysis, treatment arm (CHOP versus R-CHOP) was the major determinant of both FFS (P < .001) and OS (P < .001) for the Bcl-6(-) subset, whereas the International Prognostic Index risk group was the only significant predictor of outcome among Bcl-6(+) cases. Bcl-2 protein expression was not predictive of outcome in either group. In this study, we observed a reduction in treatment failures and death with the addition of R to CHOP in Bcl-6(-) DLBCL cases only. Our finding that Bcl-6(+) cases did not benefit from the addition of R to CHOP requires independent confirmation.},	keywords={Aged; Aged, 80 and over; Antibodies, Monoclonal/administration & dosage; Antineoplastic Combined Chemotherapy Protocols/administration & dosage/therapeutic use; Cyclophosphamide/administration & dosage; DNA-Binding Proteins/analysis/deficiency; Doxorubicin/administration & dosage; Female; Humans; Immunohistochemistry; Lymphoma, B-Cell/diagnosis/drug therapy/mortality; Lymphoma, Large B-Cell, Diffuse/diagnosis/drug therapy/mortality; Male; Middle Aged; Prednisone/administration & dosage; Prognosis; Prospective Studies; Survival Analysis; Survival Rate; Treatment Failure; Vincristine/administration & dosage},	isbn={0006-4971},	language={eng}}}@misc{RefWorks:408,	author={World Intellectual Property Organization},	title={Standards and Patents},	volume={2008},	number={03/08}}}@misc{RefWorks:159,	author={World Wide Web Consortium (W3C)},	year={2004},	month={02/10/},	title={OWL Web Ontology Language Guide},	journal={http://www.w3.org/TR/owl-guide/},}}@misc{RefWorks:227,	author={World Wide Web Consortium (W3C)},	title={Extensible Markup Language (XML)}}}@misc{RefWorks:229,	author={World Wide Web Consortium (W3C)},	title={RDF Primer},	journal={: ,}}}@misc{RefWorks:231,	author={World Wide Web Consortium (W3C)},	title={OWL Web Ontology Language Reference}}}@misc{RefWo