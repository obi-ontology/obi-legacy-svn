  <owl:Class rdf:about="&obo;TMPOBI_0000001">
    <obo:IAO_0000115 rdf:datatype="&xsd;string">One or more processes in which subject is taught key facts about a clinical trial both before deciding whether or not to participate, and throughout the study. Agents of the investivation, such as doctors and nurses involved in the trial, explain the details of the study.</obo:IAO_0000115>
    <obo:IAO_0000117 rdf:datatype="&xsd;string">Person:Alan Ruttenberg</obo:IAO_0000117>
    <obo:IAO_0000119 rdf:datatype="&xsd;string">http://clinicaltrials.gov/ct2/info/glossary#informed</obo:IAO_0000119>
    <rdfs:label rdf:datatype="&xsd;string">Informed consent process</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.ifomis.org/bfo/1.1/snap#ProcessualEntity"/>
          <owl:Restriction>
            <owl:onProperty rdf:resource="&ro;has_participant"/>
            <owl:someValuesFrom>
              <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;OBI_0000316"/>
                <owl:someValuesFrom rdf:resource="&obo;OBI_0000202"/>
              </owl:Restriction>
            </owl:someValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty rdf:resource="&ro;has_participant"/>
            <owl:someValuesFrom>
              <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;OBI_0000316"/>
                <owl:someValuesFrom rdf:resource="&obo;OBI_0000097"/>
              </owl:Restriction>
            </owl:someValuesFrom>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="&obo;TMPOBI_0000002">
    <obo:IAO_0000115 rdf:datatype="&xsd;string">A process in which a subject receives an informed consent document and agrees that they have understood it</obo:IAO_0000115>
    <obo:IAO_0000117 rdf:datatype="&xsd;string">Person:Alan Ruttenberg</obo:IAO_0000117>
    <rdfs:subClassOf>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="&obo;OBI_0000011"/>
          <owl:Restriction>
            <owl:onProperty rdf:resource="&ro;part_of"/>
            <owl:someValuesFrom rdf:resource="&obo;TMPOBI_0000001"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="&obo;TMPOBI_0000003">
    <obo:IAO_0000115 rdf:datatype="&xsd;string">A process in which the subject is made aware of which study arm they are participating in, for example whether they are receiving a placebo or a treatment with an investigational compound.</obo:IAO_0000115>
    <obo:IAO_0000117 rdf:datatype="&xsd;string">Person:Alan Ruttenberg</obo:IAO_0000117>
    <rdfs:label rdf:datatype="&xsd;string">informing subject of study arm</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.ifomis.org/bfo/1.1/span#Process"/>
          <owl:Restriction>
            <owl:onProperty rdf:resource="&ro;part_of"/>
            <owl:someValuesFrom rdf:resource="&obo;TMPOBI_0000001"/>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty rdf:resource="&ro;has_participant"/>
            <owl:someValuesFrom>
              <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;OBI_0000316"/>
                <owl:someValuesFrom rdf:resource="&obo;OBI_0000097"/>
              </owl:Restriction>
            </owl:someValuesFrom>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="&obo;TMPOBI_0000004">
    <obo:IAO_0000115 rdf:datatype="&xsd;string">A process in which an investigator is made aware of which study arm that a patient is participating in, for example whether they are receiving a placebo or a treatment with an investigational compound.</obo:IAO_0000115>
    <obo:IAO_0000117 rdf:datatype="&xsd;string">Person:Alan Ruttenberg</obo:IAO_0000117>
    <rdfs:label rdf:datatype="&xsd;string">informing investigator of subject study arm</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.ifomis.org/bfo/1.1/span#Process"/>
          <owl:Restriction>
            <owl:onProperty rdf:resource="&ro;has_participant"/>
            <owl:someValuesFrom>
              <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;OBI_0000316"/>
                <owl:someValuesFrom rdf:resource="&obo;OBI_0000202"/>
              </owl:Restriction>
            </owl:someValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty rdf:resource="&obo;OBI_0000293"/>
            <owl:someValuesFrom>
              <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;IAO_0000219"/>
                <owl:someValuesFrom>
                  <owl:Restriction>
                    <owl:onProperty rdf:resource="&OBO_REL;bearer_of"/>
                    <owl:someValuesFrom rdf:resource="&obo;OBI_0000097"/>
                  </owl:Restriction>
                </owl:someValuesFrom>
              </owl:Restriction>
            </owl:someValuesFrom>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="&obo;TMPOBI_0000005">
    <obo:IAO_0000115 rdf:datatype="&xsd;string">A single blind study execution is defined as any study execution in which the subjects are not informed of which study arm they are part of</obo:IAO_0000115>
    <obo:IAO_0000117 rdf:datatype="&xsd;string">Person:Alan Ruttenberg</obo:IAO_0000117>
    <obo:IAO_0000119 rdf:datatype="&xsd;string">http://clinicaltrials.gov/ct2/info/glossary#single</obo:IAO_0000119>
    <rdfs:label rdf:datatype="&xsd;string">single blind study execution</rdfs:label>
    <owl:intersectionOf rdf:parseType="Collection">
      <rdf:Description rdf:about="&obo;OBI_0000471"/>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:complementOf rdf:resource="&obo;TMPOBI_0000003"/>
          </owl:Class>
        </owl:allValuesFrom>
        <owl:onProperty rdf:resource="&ro;has_part"/>
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>

  <owl:Class rdf:about="&obo;TMPOBI_0000006">
    <obo:IAO_0000115 rdf:datatype="&xsd;string">A double blind study execution is defined as any study execution in which neither the subjects nor the investigators are informed of which study arm the subjects are part of</obo:IAO_0000115>
    <obo:IAO_0000117 rdf:datatype="&xsd;string">Person:Alan Ruttenberg</obo:IAO_0000117>
    <obo:IAO_0000119 rdf:datatype="&xsd;string">http://clinicaltrials.gov/ct2/info/glossary#double</obo:IAO_0000119>
    <rdfs:label rdf:datatype="&xsd;string">double blind study execution</rdfs:label>
    <owl:intersectionOf rdf:parseType="Collection">
      <rdf:Description rdf:about="&obo;OBI_0000471"/>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:complementOf>
              <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                  <rdf:Description rdf:about="&obo;TMPOBI_0000003"/>
                  <rdf:Description rdf:about="&obo;TMPOBI_0000004"/>
                </owl:unionOf>
              </owl:Class>
            </owl:complementOf>
          </owl:Class>
        </owl:allValuesFrom>
        <owl:onProperty rdf:resource="&ro;has_part"/>
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>

